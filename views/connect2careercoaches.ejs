    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Aadia- Rejuvenate and Re-energize</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
   <link rel="stylesheet" type="text/css" href="css/style.css">

  
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Carrois+Gothic+SC" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script type="text/JavaScript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.0/socket.io.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.0/socket.io.dev.js"></script>

    <script src="../js/bootstrap.min.js"></script>
    <script src="../js/script.js"></script>
</head>
<div id="page">

       <header>
                <div id="heading"> 
                  <h1>Aadia -- Holistic App</h1>
                 <p>Rejuvenate and Re-energize</p>
            </div>   
                <div id="svg" > 
                <h1>A practical guide to growth</h1>
               </div>        
        </header>



<div class="topheading">
<div class="homebtn">
    <a  href="/lifeskills" class="btn btn-dark btn-lg" >Home</a>
</div>

    <div class="navbtn">
    <a style="font-size:20px;" id="logout" class="logout" href="/logout">logout</a>
                        <p style="font-size:14px;color:black">
                            <% if(user.google.email) { %>
                            <%=user.google.email%>
                             <% } %>

                              <% if(user.facebook.email) { %>
                            <%=user.facebook.email%>
                             <% } %>

                               <% if(user.local.email) { %>
                            <%=user.local.email%>
                             <% } %>
                        </p>

      </div>
</div>

               

         <div class="jumbotron" style="padding:0;margin-left:10px">
    <div class="rowpage row" style="padding:0;">
      <div class="col-sm-2 col-xs-2" style="padding:0;margin-left:20px">
          <ul class="nav nav-list">
            <li><label class="tree-toggler nav-header">Career Coaches</label>
                <ul class="menu vertical"  style="line-height:2em">
                    <%if (typeof results !== 'undefined' && results ){%>
                    <% for(var i=0; i<results.length; i++) {%>
                    <li ><%=results[i].name%><br></li>
                    <%}%>
                    <%} else {%>
                    <%if (typeof message !== 'undefined' && message ){%>
                    <div class="col-sm-4" style="background:#CD5C5C"><p><%=message%></p></div>
                    <%}%>
                    <% } %>
                   </ul>
               </li>
           </ul>
      </div>
     <div class="col-sm-9 col-xs-9">
        <div id="Leave message to any of the coaches">
          <h2>Drop a Message </h2>
         <div id="chat-window">
          <div id="output"></div>
          <div id="feedback"></div>
       </div>
            <input id="handle" type="text" placeholder="Handle" value= "<% if(user.google.email) { %>
                           <%=user.google.name%>
                             <% } %>

                              <% if(user.facebook.email) { %>
                           <%=user.facebook.name%>
                             <% } %>

                               <% if(user.local.email) { %>
                                  <%=user.local.email.substring(0,2).trim()%>

                             <% } %>"/>
            <input id="message" type="text" placeholder="Message" />
            <button id="send">Send</button>
          </div>
      </div>   
               
</div>

         

      <script src="../js/chat.js"></script>
</body>
</html>