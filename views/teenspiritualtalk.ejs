                    $('.toggle').click(function () {
                        var $toggled = $(this).attr('href');
                       
                        $($toggled).siblings(':visible').hide();
                        $($toggled).toggle("slide", {
                            direction: 'up'
                        }, 1000);
                        $($toggled).toggle();
