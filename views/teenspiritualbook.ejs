     <script>
 jQuery(document).ready(function ($) {
      $('#myTable').DataTable();
  });

      </script>

<div id="page">

       <header>
                <div id="heading"> 
                  <h1>Aadia -- Holistic App</h1>
                 <p>Rejuvenate and Re-energize</p>
            </div>   
                <div id="svg" > 
                <h1>A practical guide to growth</h1>
               </div>        
        </header>



<div class="topheading">
<div class="homebtn">
    <a  href="/teenselfesteemthanks" class="btn btn-dark btn-lg" >Home</a>
</div>

    <div class="navbtn">
    <a style="font-size:20px;" id="logout" class="logout" href="/logout">logout</a>
                        <p style="font-size:14px;color:black">
                            <% if(user.google.email) { %>
                            <%=user.google.email%>
                             <% } %>

                              <% if(user.facebook.email) { %>
                            <%=user.facebook.email%>
                             <% } %>

                               <% if(user.local.email) { %>
                            <%=user.local.email%>
                             <% } %>
                        </p>

      </div>
</div>



   <div class="jumbotron" style="padding:0;margin-left:10px">
    <div class="rowpage row" style="padding:0;">
      <div class="col-sm-2 col-xs-2" style="padding:0;">
       
      </div>
       
  
               

         <div class="col-sm-9 col-xs-9" >
          

                <div class="table-responsive">



                    <table class="table table-bordered" id="myTable">
                        <thead>
                            <tr>
                                <th style="text-align:center">
                                   <h5></h5><p>Spiritual books</p> 
                                </th>

                                <th style="text-align:center">
                                   <h5></h5><p>Author</p> 
                                </th>

                               
                                 <th style="text-align:center">
                                   <h5></h5><p>Amazon Link</p> 
                                </th>
                            </tr>


                        </thead>
                        <tbody>



                            <%  for(var i=0; i<spbooksdata.booklist.length; i++) {%>

                                <tr>
                                    <td>
                                        <a class="toggle" href="#">
                                            <%=spbooksdata.booklist[i].bookTitle%>
                                              
                                    </td>
                                     <td>
                                        <a class="toggle" href="#">
                                            <%=spbooksdata.booklist[i].bookAuthor%>
                                              
                                    </td>
                                     <td>
                                        <a class="toggle" href="<%=spbooksdata.booklist[i].amazonlink%>" target="_blank">
                                            <%=spbooksdata.booklist[i].amazonlink%>
                                              
                                    </td>
                                </tr>
                                <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
             </div>
        </div>
        

            </div>
